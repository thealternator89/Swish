{
  "name": "swish-clip",
  "version": "1.0.0",
  "description": "Light swish GUI for interacting with the clipboard",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "copyfiles assets/**/* dist",
    "clean": "rimraf dist",
    "package": "electron-packager . --overwrite --prune --out=release-builds",
    "package:mac": "npm run package -- --icon=electron/assets/icons/mac/icon.icns",
    "prestart": "npm run build",
    "start": "electron ."
  },
  "author": "Mark Benson <thealternator89@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "copyfiles": "^2.4.1",
    "electron": "^15.3.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "-": "0.0.1",
    "swish-base": "file:../Base"
  }
}
