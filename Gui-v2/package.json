{
  "name": "swish-desktop",
  "productName": "swish-desktop",
  "description": "Simple and powerful data transformation tool",
  "author": "Mark Benson <thealternator89@gmail.com>",
  "version": "2.0.0-alpha",
  "scripts": {
    "build": "npm run build:ng && npm run build:electron",
    "build:ng": "ng build --base-href ./ --prod",
    "build:electron": "tsc --p electron",
    "postbuild:electron": "copyfiles electron/assets/**/* electron/dist",
    "clean": "npm run clean:ng && npm run clean:electron",
    "clean:ng": "rimraf dist",
    "clean:electron": "rimraf electron/dist",
    "dev:electron": "cross-env SWISH_DEVELOPMENT='1' npm run buildstart:electron",
    "dev:ng": "ng serve",
    "dev": "concurrently \"npm:dev:ng\" \"npm:dev:electron\"",
    "package": "electron-packager . --overwrite --prune --out=release-builds",
    "package:mac": "npm run package -- --icon=electron/assets/icons/mac/icon.icns",
    "start": "electron .",
    "buildstart": "npm run build && npm run start",
    "buildstart:electron": "npm run build:electron && npm run start",
    "pack-installer": "electron-installer-windows --src release-builds/swish-desktop-win32-x64 --dest release-builds/installers/"
  },
  "main": "electron/dist/electron/main.js",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/cdk": "^11.0.4",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/material": "^11.0.4",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@ng-util/monaco-editor": "^11.1.1",
    "clipboard": "^2.0.11",
    "marked": "^4.3.0",
    "monaco-editor": "^0.21.2",
    "ngx-markdown": "^11.2.0",
    "prismjs": "^1.29.0",
    "rxjs": "~6.6.0",
    "swish-base": "file:../Base",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "^11.1.2",
    "@angular/compiler-cli": "~11.0.9",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^7.6.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "electron": "^16.2.8",
    "electron-builder": "^24.4.0",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
